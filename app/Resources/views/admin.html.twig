<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html"; charset=utf-8" />
    <title>Admin {% block title %}Job Manager{% endblock %}</title>
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    {% block stylesheets %}

        {% stylesheets output='css/admin.css' filter='cssrewrite, ?yui_css'
        '@JobmanagerAdminBundle/Resources/public/css/reset.css'
        '@JobmanagerAdminBundle/Resources/public/css/admin.css'
        '@JobmanagerAdminBundle/Resources/public/css/bootstrap.css'
        '@JobmanagerAdminBundle/Resources/public/css/font-awesome.css'
        '@JobmanagerAdminBundle/Resources/public/css/ionicons.css'
        '@JobmanagerAdminBundle/Resources/public/css/AdminLTE.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
        {% endstylesheets %}

    {% endblock %}
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
    <link rel="shortcut icon" href="<h2> asset('favicon.ico') </h2>" />
</head>
<body class="skin-blue">
    {% block head %}
        <header class="header">
            <a href="/app_dev.php" class="logo">
                <!-- Add the class icon to your logo image or logo icon to add the margining -->
                Job Manager
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>

            </nav>
        </header>
    {% endblock %}


    <div class="wrapper row-offcanvas row-offcanvas-left">

        {% block nav %}

        {% endblock %}

        {% block content %}

        {% endblock %}

    </div><!-- ./wrapper -->

    {% block javascript %}


    {% endblock %}
</body>
</html>