<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html"; charset=utf-8" />
    <title>Admin {% block title %}Job Manager{% endblock %}</title>
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    {% block stylesheets %}
        {% stylesheets output='css/admin.css' filter='cssrewrite, ?yui_css'
        '@JobmanagerAdminBundle/Resources/public/css/reset.css'
        '@JobmanagerAdminBundle/Resources/public/css/admin.css'
        '@JobmanagerAdminBundle/Resources/public/css/bootstrap.css'
        '@JobmanagerAdminBundle/Resources/public/css/font-awesome.css'
        '@JobmanagerAdminBundle/Resources/public/css/ionicons.css'
        '@JobmanagerAdminBundle/Resources/public/css/morris/morris.css'
        '@JobmanagerAdminBundle/Resources/public/css/jvectormap/jquery-jvectormap-1.2.2.css'
        '@JobmanagerAdminBundle/Resources/public/css/fullcalendar/fullcalendar.css'
        '@JobmanagerAdminBundle/Resources/public/css/daterangepicker/daterangepicker-bs3.css'
        '@JobmanagerAdminBundle/Resources/public/css/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css'
        '@JobmanagerAdminBundle/Resources/public/css/datatables/dataTables.bootstrap.css'
        '@JobmanagerAdminBundle/Resources/public/css/AdminLTE.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
        {% endstylesheets %}

    {% endblock %}
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
    <link rel="shortcut icon" href="<h2> asset('favicon.ico') </h2>" />
</head>
<body class="skin-blue">
    {% block head %}
        <header class="header">
            <a href="index.html" class="logo">
                <!-- Add the class icon to your logo image or logo icon to add the margining -->
                Job Manager
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-right">
                    <ul class="nav navbar-nav">
                        <li>
                            <a href="">Site public</a>
                        </li>
                        <li>
                            <a href="">Site DÃ©connexion</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
    {% endblock %}


    <div class="wrapper row-offcanvas row-offcanvas-left">

        {% block nav %}

        {% endblock %}

        {% block content %}

        {% endblock %}

    </div><!-- ./wrapper -->

    {% block javascript %}
        <!-- jQuery 2.0.2 -->
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.2/jquery.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
        {% javascripts output='js/main.js' filter='cssrewrite, ?yui_js'
        '@JobmanagerAdminBundle/Resources/public/js/jquery-ui-1.10.3.min.js'
        '@JobmanagerAdminBundle/Resources/public/js/bootstrap.min.js'
        '@JobmanagerAdminBundle/Resources/public/js/plugins/morris/morris.min.js'
        '@JobmanagerAdminBundle/Resources/public/js/plugins/sparkline/jquery.sparkline.min.js'
        '@JobmanagerAdminBundle/Resources/public/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js'
        '@JobmanagerAdminBundle/Resources/public/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js'
        '@JobmanagerAdminBundle/Resources/public/js/plugins/fullcalendar/fullcalendar.min.js'
        '@JobmanagerAdminBundle/Resources/public/js/plugins/jqueryKnob/jquery.knob.js'
        '@JobmanagerAdminBundle/Resources/public/js/plugins/daterangepicker/daterangepicker.js'
        '@JobmanagerAdminBundle/Resources/public/js/plugins/iCheck/icheck.min.js'
        '@JobmanagerAdminBundle/Resources/public/js/plugins/datatables/jquery.dataTables.js'
        '@JobmanagerAdminBundle/Resources/public/js/plugins/datatables/dataTables.bootstrap.js'
        '@JobmanagerAdminBundle/Resources/public/js/AdminLTE/app.js'
        '@JobmanagerAdminBundle/Resources/public/js/AdminLTE/dashboard.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>

        {% endjavascripts %}
    {% endblock %}
</body>
</html>